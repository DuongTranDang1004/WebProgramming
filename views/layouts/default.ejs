<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%- title %></title>

    <!-- Bootstrap 3.1.0 (for pre-built modal layout) -->

    <link
      href="//netdna.bootstrapcdn.com/bootstrap/3.1.0/css/bootstrap.min.css"
      rel="stylesheet"
      id="bootstrap-css"
    />
    <!-- <script src="//netdna.bootstrapcdn.com/bootstrap/3.1.0/js/bootstrap.min.js"></script>
    <script src="//code.jquery.com/jquery-1.11.1.min.js"></script> -->
    <!-- BootStrap 5.3.3 -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
    <!-- Font Awesome 5 CDN -->
    <link
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css"
      rel="stylesheet"
    />

    <!-- global css for all the pages that use this layout  -->
    <link rel="stylesheet" href="/css/globalStyling/global.css" />
    <!-- custom css sheet for specific pages (pass in as para when render as app.js ) -->
    <!--  (auto add leading '/') -->

    <% if (typeof pageStylesheet !== 'undefined') { %>
    <link rel="stylesheet" href="<%= pageStylesheet %>" />
    <% } %>

    <!-- import library for icons -->
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
    />
  </head>
  <body>
    <%- include('../partials/header') %>
    <main class="main-content">
      <%- body %>
      <!-- This will include the content of the specific EJS file being rendered at the route in app.js -->
    </main>

    <%- include('../partials/footer') %>
    <!-- bootstrap script -->
    <!-- First include jquery js -->
    <script src="//code.jquery.com/jquery-1.12.0.min.js"></script>
    <script src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
    <script
      src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js"
      integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js"
      integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy"
      crossorigin="anonymous"
    ></script>

    <script src="https://unpkg.com/@themesberg/flowbite@1.1.1/dist/flowbite.bundle.js"></script>

    <!-- global javascript (to handle event for header nav bar)  -->
    <!-- <script src="js/clientSideProcessing/globalProcessing.js"></script> -->

    <!-- Custom JavaScript for specific pages (auto add leading '/') (pass as pageScript property) -->
    <% if (Array.isArray(pageScripts)) { %> <%
    pageScripts.forEach(function(script) { %>
    <script src="<%= script.startsWith('/') ? script : '/' + script %>"></script>
    <% }); %> <% } %>
  </body>
</html>
