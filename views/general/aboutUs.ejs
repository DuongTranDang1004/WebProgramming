    <!-- Duong Avatar Container -->
    <div class="container">
      <div class="span3 well text-center">
        <a href="#aboutModal" data-toggle="modal" data-target="#DuongModal">
          <img
            src="img/teamMemberAvatar/DuongAvatar.jpg"
            alt="Duong Avatar"
            name="aboutme"
            width="140"
            height="140"
            class="img-circle"
          />
        </a>
        <h3>Duong Tran</h3>
        <h4>Front-end developer</h4>
